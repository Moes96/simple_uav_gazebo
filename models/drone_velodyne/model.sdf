<?xml version="1.0"?>
<sdf version='1.6'>
    <model name="drone">
        <static>false</static>
        <!--Base of the drone-->
        <link name='drone_base'>
        <inertial>
            <pose> 0.000013 0.000013 -0.0383399</pose>
            <mass> 1.5 </mass>
            <inertia>
                <ixx>0.0347563</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.0458929</iyy>
                <iyz>0</iyz>
                <izz>0.0977</izz>
            </inertia>
        </inertial>
            <pose>0 0 0.3 0 -0 0</pose>
            <collision name='collision'>
                <geometry>
                    <box>
                        <size>.3 .3 .1</size>
                    </box>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>model://drone/meshes/drone_base.dae</uri>
                        <scale>0.033 0.033 0.033</scale>
                    </mesh>
                </geometry>
            </visual>
        </link>
        <!-- Propellers -->
        <link name='elica_north'>
	<inertial>
            <pose> 0.000013 0.000013 -0.0383399</pose>
            <mass> 0.005 </mass>
            <inertia>
                <ixx>9.75e-07</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000273104</iyy>
                <iyz>0</iyz>
                <izz>0.000274004</izz>
            </inertia>
        </inertial>
            <pose>.116 .116 0.3</pose>
            <collision name='collision'>
                <geometry>
                    <cylinder>
              		<radius>.1</radius>
              		<length>.05</length>
            	</cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>model://drone/meshes/elica.obj</uri>
                        <scale>0.025 0.025 0.025</scale>
                    </mesh>
                </geometry>
            </visual>
        </link>
        <link name='elica_south'>
	<inertial>
            <pose> 0.000013 0.000013 -0.0383399</pose>
            <mass> 0.005 </mass>
            <inertia>
                <ixx>9.75e-07</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000273104</iyy>
                <iyz>0</iyz>
                <izz>0.000274004</izz>
            </inertia>
        </inertial>
            <pose>-.116 -.116 0.3</pose>
            <collision name='collision'>
                <geometry>
                    <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>model://drone/meshes/elica.obj</uri>
                        <scale>0.025 0.025 0.025</scale>
                    </mesh>
                </geometry>
            </visual>
        </link>
        <link name='elica_east'>
	<inertial>
            <pose> 0.000013 0.000013 -0.0383399</pose>
            <mass> 0.005 </mass>
            <inertia>
                <ixx>9.75e-07</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000273104</iyy>
                <iyz>0</iyz>
                <izz>0.000274004</izz>
            </inertia>
        </inertial>
            <pose>.116 -.116 0.3</pose>
            <collision name='collision'>
                <geometry>
                    <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>model://drone/meshes/elica.obj</uri>
                        <scale>0.025 0.025 0.025</scale>
                    </mesh>
                </geometry>
            </visual>
        </link>
        <link name='elica_west'>
	<inertial>
            <pose> 0.000013 0.000013 -0.0383399</pose>
            <mass> 0.005 </mass>
            <inertia>
                <ixx>9.75e-07</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.000273104</iyy>
                <iyz>0</iyz>
                <izz>0.000274004</izz>
            </inertia>
        </inertial>
            <pose>-.116 .116 0.3</pose>
            <collision name='collision'>
                <geometry>
                    <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>model://drone/meshes/elica.obj</uri>
                        <scale>0.025 0.025 0.025</scale>
                    </mesh>
                </geometry>
            </visual>
        </link>

        <!--joints for the propellers-->
        <joint type="revolute" name="north_blade_hinge">
            <pose>0 0 0</pose>
            <child>elica_north</child>
            <parent>drone_base</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

        <joint type="revolute" name="south_blade_hinge">
            <pose>0 0 0</pose>
            <child>elica_south</child>
            <parent>drone_base</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

        <joint type="revolute" name="east_blade_hinge">
            <pose>0 0 0</pose>
            <child>elica_east</child>
            <parent>drone_base</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

        <joint type="revolute" name="west_blade_hinge">
            <pose>0 0 0</pose>
            <child>elica_west</child>
            <parent>drone_base</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

        <!-- Plugin space
        <plugin name="upforce" filename="libUpForce.so">
            <final_position>10 10 10</final_position>
        </plugin>
        <plugin name="DroneCS" filename="libDroneCS2.so"/>-->

	
	<!-- Servo and Velodyne plugins -->
	<include>
      		<uri>model://servo_ax12a</uri>
      		<pose>0.09 0.00 0.335 0 0 1.58</pose>
    	</include>
    	<joint name='servo_ax12a_joint' type='fixed'>
		  <child>servo_ax12a::Body</child>
		  <parent>drone_base</parent>
    	</joint>
    

	<include>
		 <uri>model://VLP-16</uri>
		 <pose>0.175 0.00 0.3485 0 -0 0</pose>  <!-- 0.16 0 0.08 0 0 0 -->
	</include>
	<joint name='vlp16_joint' type='fixed'>
		 <child>VLP-16::velodyne_base_link</child>
		 <parent>servo_ax12a::Wheel</parent>
    	</joint>
    </model>
</sdf>
